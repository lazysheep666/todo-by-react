{
    "root": true,
    "parser": "babel-eslint",
    "extends": [
      "standard",
      "plugin:react/recommended"
    ],
    "plugins": [
        "standard",
        "html",
        "react"
    ],
    "env": {
        "browser": true,
        "node": true,
        "es6": true,
        "jquery": true
    },
    "rules": {
      "space-before-function-paren": ["error", "never"]
    }
}